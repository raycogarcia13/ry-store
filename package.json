{
  "name": "electron-react-app",
  "version": "1.0.0",
  "description": "Electron + React + TypeScript application",
  "main": "dist/main/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently -k \"cross-env NODE_ENV=development webpack serve --config webpack.config.js --mode development\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development electron .\"",
